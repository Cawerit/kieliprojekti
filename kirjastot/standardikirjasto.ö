tyyppi(a) =
  standardikirjasto(0, "tyyppi", a)

infiksi 5 + (a, b) =
  standardikirjasto(0, "summaa", a, b)

infiksi 5 - (a, b) =
  standardikirjasto(0, "vahenna", a, b)

infiksi 10 * (a, b) = standardikirjasto(0, "kerro", a, b)

infiksi 10 / (a, b) = standardikirjasto(0, "jaa", a, b)

infiksi 5 ++ (a, b) = standardikirjasto(0, "yhdistaTekstit", a, b)

infiksi 10 % (a, b) = standardikirjasto(0, "jakojaannos", a, b)

infiksi 11 ^ (a, b) = standardikirjasto(0, "potenssiinKorotus", a, b)

on(a, b) = standardikirjasto(0, "on", a, b)

infiksi 2 on: (a, b) = standardikirjasto(0, "on", a, b)

näytä(teksti, tila) = standardikirjasto(0, "nayta", teksti, tila)

tämä(arvo) = standardikirjasto(0, "tama", arvo)

eiMitään = standardikirjasto(1, "eiMitään")

pituus(jono) = standardikirjasto(0, "pituus", jono)

jos(ehto, arvo) = standardikirjasto(0, "jos", ehto, arvo)

infiksi 1 muutoin: (ehkäArvo, taiSitten) = standardikirjasto(0, "muutoin", ehkäArvo, taiSitten)

muutoin(ehkäArvo, taiSitten) = standardikirjasto(0, "muutoin", ehkäArvo, taiSitten)

kysy(kysymys, tilanMuokkaus) = standardikirjasto(0, "kysy", kysymys, tilanMuokkaus)

identiteetti(a) = a
